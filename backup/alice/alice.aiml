<aiml version="1.0">

    <category>
        <pattern>_ ALICE</pattern>
        <template>
            <think>
                <set name="savedtopic">
                    <get name="topic"/>
                </set>
                <set name="topic">TERMINA COM ALICE</set>
                <set name="response">falso</set>
                <set name="resposta">
                    <srai>
                        <star/>
                        BOTNAME
                    </srai>
                </set>
            </think>
            <condition name="matched">

                <li value="true">
                    <get name="resposta"/>
                </li>
                <li>
                    <srai>
                        <star/>
                    </srai>
                </li>
            </condition>
            <think>
                <set name="topic">
                    <get name="savedtopic"/>
                </set>
            </think>
        </template>
    </category>

